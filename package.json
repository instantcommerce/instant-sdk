{
  "name": "instant-cli",
  "version": "0.0.0",
  "license": "MIT",
  "author": "cli@instantcommerce.io",
  "bin": "dist/cli.js",
  "type": "module",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "compile": "rm -rf dist && tsup src/cli.ts --format esm && chmod +x dist/cli.js",
    "build": "tsc --noEmit && npm run compile",
    "build:dev": "npm run compile && cp .env dist/.env",
    "start": "npm run build && dist/cli.js",
    "dev": "npm run build:dev && cp .env dist/.env && dist/cli.js",
    "pretest": "yarn build",
    "test": "xo && ava",
    "codegen": "graphql-codegen -r dotenv/config"
  },
  "files": [
    "dist/cli.js"
  ],
  "dependencies": {
    "arr-rotate": "^1.0.0",
    "axios": "^1.1.3",
    "compression": "^1.7.4",
    "conf": "^10.2.0",
    "connect": "^3.7.0",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "ink": "^3.2.0",
    "meow": "^9.0.0",
    "open": "^8.4.0",
    "react": "^17.0.2",
    "serve-static": "^1.15.0",
    "vite": "^3.2.3",
    "yargs": "^17.6.0"
  },
  "ava": {
    "typescript": {
      "extensions": [
        "tsx"
      ],
      "rewritePaths": {
        "src/": "dist/"
      }
    }
  },
  "xo": {
    "extends": "xo-react",
    "rules": {
      "react/prop-types": "off"
    }
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.7.4",
    "@graphql-codegen/typescript-graphql-request": "^4.5.6",
    "@graphql-codegen/typescript-operations": "^2.5.4",
    "@graphql-codegen/typescript-react-query": "^4.0.2",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/compression": "^1.7.2",
    "@types/connect": "^3.4.35",
    "@types/lodash": "^4.14.186",
    "@types/react": "^18.0.21",
    "@types/serve-static": "^1.15.0",
    "@types/yargs": "^17.0.13",
    "ava": "^4.3.3",
    "chalk": "^4.1.2",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ink-testing-library": "^2.1.0",
    "tsup": "^6.4.0",
    "typescript": "^4.8.4",
    "xo": "^0.39.1"
  }
}
