{
  "name": "vite-plugin-instant-sdk",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/vite-client/client.d.ts",
      "import": "./dist/vite-client/client.js"
    }
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:compile:esm",
    "build:clean": "rm -rf dist && mkdir dist",
    "build:compile:esm": "cd src && yarn tsc --build tsconfig.json",
    "format": "prettier --write ."
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "hashids": "2.2.10",
    "postcss-plugin-instant-sdk": "*",
    "react-refresh": "^0.14.0"
  },
  "devDependencies": {
    "tsconfig": "*"
  },
  "peerDependencies": {
    "vite": "^3.0.0"
  }
}
