{
  "name": "@instantcommerce/cli",
  "version": "0.0.1",
  "author": "Instant Commerce B.V.",
  "license": "MIT",
  "bin": {
    "instant": "dist/cli.js"
  },
  "type": "module",
  "engines": {
    "node": ">=16.x <=18.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "compile": "tsup && chmod +x dist/cli.js",
    "build": "npm run compile && mkdir -p ./dist/preview/ && cp -r ../preview/dist ./dist/preview/ && cp -r ../../templates ./dist/",
    "build:dev": "npm run compile && cp .env dist/.env",
    "start": "npm run build && dist/cli.js",
    "dev": "npm run build:dev && cp .env dist/.env",
    "pretest": "yarn build",
    "test": "xo && ava"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@instantcommerce/vite-plugin-sdk": "*",
    "@vitejs/plugin-react": "2.1.0",
    "arr-rotate": "1.0.0",
    "axios": "1.1.3",
    "compression": "1.7.4",
    "conf": "10.2.0",
    "connect": "3.7.0",
    "dot-prop": "7.2.0",
    "dotenv": "16.0.3",
    "glob": "8.0.3",
    "graphql": "16.6.0",
    "graphql-request": "5.0.0",
    "humanize-string": "3.0.0",
    "ink": "3.2.0",
    "meow": "9.0.0",
    "open": "8.4.0",
    "react": "17.0.2",
    "serve-static": "1.15.0",
    "terminal-link": "3.0.0",
    "types": "*",
    "vite": "3.2.3",
    "yargs": "17.6.0"
  },
  "ava": {
    "typescript": {
      "extensions": [
        "tsx"
      ],
      "rewritePaths": {
        "src/": "dist/"
      }
    }
  },
  "xo": {
    "extends": "xo-react",
    "rules": {
      "react/prop-types": "off"
    }
  },
  "devDependencies": {
    "@ava/typescript": "3.0.1",
    "@sindresorhus/tsconfig": "3.0.1",
    "@types/compression": "1.7.2",
    "@types/connect": "3.4.35",
    "@types/lodash": "4.14.186",
    "@types/react": "18.0.21",
    "@types/serve-static": "1.15.0",
    "@types/yargs": "17.0.13",
    "ava": "4.3.3",
    "chalk": "4.1.2",
    "eslint-config-xo-react": "0.27.0",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "ink-testing-library": "2.1.0",
    "tsup": "6.4.0",
    "typescript": "4.8.4",
    "xo": "0.39.1"
  }
}
